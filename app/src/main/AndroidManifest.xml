<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.thesparkbank">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="The Spark Bank"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".CustomersActivity"></activity>
        <activity android:name=".ProfileActivity"></activity>
        <activity android:name=".TransferActivity"></activity>
        <activity android:name=".HistoryActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>